{% extends '_layout.twig' %}

{% block main_content %}

<h1>All Planets </h1>

<ul>
    {% for planet in planets %}
    <li>
    <a a href="/planets/{{planet.id}}">#{{planet.id}} -- <strong>{{planet.name}}</strong>
    </a>
    </li>
    {% endfor %}
</ul>

<p>
<a a href="/planets/new">Create a new Planet</a>

</p>

{% endblock %}